package diag.reiter2;

/**
 * A <code>DiagnosisIOException</code>, i.e., a diagnosis IO exception, 
 * is an exception related to Input / Output operations.  
 * It is more general than {@link java.util.IOException}.  
 */
public class DiagnosisIOException extends Exception {
    
    public DiagnosisIOException(String message) {
        super(message);
    }
}
